import inquirer from "inquirer"
const guess = await inquirer.prompt(
    [{message:"guess a number between 10 to 20",name: "number",type:"number"},]
)
while (guess.number != 13){
    const guess = await inquirer.prompt(
        [{message:"guess a number between 10 to 20",name: "number",type:"number"},]
    )
if (guess.number===13) {
    console.log("congrats you chose correct number ")
    break } else {
    console.log("sorry try agian")
}}
